{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/auth/signup", "dest": "signup.js" },
    { "src": "/api/auth/login", "dest": "login.js" },
    { "src": "/api/post", "dest": "post.js" },
    { "src": "/(.*)", "dest": "server.js" }
  ]
}
